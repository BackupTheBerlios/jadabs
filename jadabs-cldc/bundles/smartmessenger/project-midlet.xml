<?xml version="1.0" encoding="UTF-8"?>

<project>

	<extend>${basedir}/../../project.xml</extend>

	<groupId>jadabs</groupId>
	<artifactId>smartmessenger</artifactId>
	<name>Jadabs-SmartMessenger</name>

	<shortDescription>SmartMessenger for J2ME-JXME</shortDescription>

  	<dependencies>  	
  	
  
    	<dependency>
      		<groupId>osgi</groupId>
      		<artifactId>osgi-j2me</artifactId>
      		<version>${pom.currentVersion}</version>
      		<properties>
	      		<midlet>true</midlet>  
	      	</properties>
		</dependency>

      <dependency>
      		<groupId>jadabs</groupId>
      		<artifactId>jxme-osgi-j2me</artifactId>
      		<version>${pom.currentVersion}</version>
      		<properties>
	      		<midlet>true</midlet>  
	      	</properties>
		</dependency>
      
      <dependency>
      		<groupId>mservices</groupId>
      		<artifactId>smsservice</artifactId>
      		<version>${pom.currentVersion}</version>
      		<properties>
	      		<midlet>true</midlet>  
	      	</properties>
		</dependency>
      
      <dependency>
      		<groupId>mservices</groupId>
      		<artifactId>smsgateway</artifactId>
      		<version>${pom.currentVersion}</version>
      		<properties>
	      		<midlet>true</midlet>  
	      	</properties>
		</dependency>            
      		
		<dependency>
      		<groupId>jadabs</groupId>
      		<artifactId>jxme-bt-j2me</artifactId>
      		<version>${pom.currentVersion}</version>
      		<properties>
	      		<midlet>true</midlet>  
	      	</properties>
		</dependency>        
	
    	<dependency>
      		<groupId>jadabs</groupId>
      		<artifactId>log4j-j2me</artifactId>
      		<version>${pom.currentVersion}</version>
      		<properties>
	    			<midlet>true</midlet>    
      	   </properties>
		</dependency>
		
      	<dependency>
      		<groupId>j2me</groupId>
      		<artifactId>wma</artifactId>
      		<version>1.1</version>
      		<properties>
      			<dependencyType>runtime</dependencyType>
      		</properties>
		</dependency>			
      
		<dependency>
      		<groupId>j2me</groupId>
      		<artifactId>midpapi</artifactId>
      		<version>2.0</version>
		</dependency>	
		
		<dependency>
      		<groupId>j2me</groupId>
      		<artifactId>btapi</artifactId>
      		<version>1.0</version>
      		<properties>
      			<dependencyType>runtime</dependencyType>
      		</properties>
		</dependency>

		
  	</dependencies>
  
</project>
