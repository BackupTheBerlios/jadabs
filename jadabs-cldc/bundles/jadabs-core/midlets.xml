 <descriptor xmlns="http://wlab.ethz.ch/maven/plugins/maven-antenna-plugin">
 
 	<suite>Jadabs-Core Bundle</suite>
  	<vendor>Andreas Frei, Rene Mueller, ETH Zurich</vendor> 
  	<version>1.0.0</version> 
  	<midlets>
  		<midlet class="ch.ethz.jadabs.core.JadabsCoreMIDlet" 
  		        name="Jadabs-Core"/>  		
  	</midlets>

   <attributes>
      <attribute name="ch.ethz.jadabs.jxme.peeralias" value="Nokia6600"/>
