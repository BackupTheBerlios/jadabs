 <descriptor xmlns="http://wlab.ethz.ch/maven/plugins/maven-antenna-plugin">

 	<suite>JXMEWS</suite>
  	<vendor>A.Frei</vendor>
  	<version>1.0.0</version>
  	<midlets>
  		<midlet class="ch.ethz.jadabs.jxmews.JxmeWSMIDlet"
  		        name="JXME-WS"/>
  	</midlets>

   <attributes>
      <attribute name="ch.ethz.jadabs.jxme.peeralias" value="Nokia6600"/>
      <attribute name="log4j.priority" value="DEBUG"/>
      
      <attribute name="ch.ethz.jadabs.microservices.bundleport" value="1235"/>
      <attribute name="MIDlet-Push-1" value="socket://:1235, ch.ethz.jadabs.sbbws.midlet.SBBMIDlet, 127.0.0.1"/>
   </attributes>
</descriptor>