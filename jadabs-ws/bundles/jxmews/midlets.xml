<descriptor xmlns="http://wlab.ethz.ch/maven/plugins/maven-antenna-plugin">

 	<suite>JXMEWS</suite>
  	<vendor>A.Frei</vendor>
  	<version>1.0.0</version>
  	<midlets>
  		<midlet class="ch.ethz.jadabs.jxmews.JxmeWSMIDlet"
  		        name="JXME-WS"/>
  	</midlets>

   <attributes>
      <attribute name="ch.ethz.jadabs.jxme.peeralias" value="Nokia6600"/>
      <attribute name="log4j.priority" value="DEBUG"/>
      
      <attribute name="ch.ethz.jadabs.microservices.bundleport" value="5435"/>
      <attribute name="MIDlet-Push-1" value="socket://:5435, ch.ethz.jadabs.jxmews.JxmeWSMIDlet, 127.0.0.1"/>
   </attributes>
</descriptor>