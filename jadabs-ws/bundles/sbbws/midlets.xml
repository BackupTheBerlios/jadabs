 <descriptor xmlns="http://wlab.ethz.ch/maven/plugins/maven-antenna-plugin">

 	<suite>SBBWS</suite>
  	<vendor>Stefan Vogt, Franz Maier</vendor>
  	<version>1.0.0</version>
  	<midlets>
  		<midlet class="ch.ethz.jadabs.sbbws.midlet.SBBMIDlet"
  		        name="SBBWS"/>
  	</midlets>

   <attributes>
      <attribute name="ch.ethz.jadabs.jxme.peeralias" value="Nokia6600"/>
      <attribute name="log4j.priority" value="DEBUG"/>
      
      <attribute name="ch.ethz.jadabs.microservices.bundleport" value="1234"/>
      <attribute name="MIDlet-Push-1" value="socket://:1234, ch.ethz.jadabs.sbbws.midlet.SBBMIDlet, 127.0.0.1"/>
   </attributes>
</descriptor>